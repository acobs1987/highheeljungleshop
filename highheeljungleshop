                        var res = pattern.test(str);
                        //console.log("result "+res);
                        if(res){
                          flag=2;
                            return;
                        }
                        else{
                          //console.log("in else "+str);
                          $m(this).parent().addClass("vichitra");
                            flag = 0;
                            return false;
                        }
                      //}
                      });
                      //console.log("flag "+flag);
                      if(flag==2){
