<script>
  //console.log("here");
var mlvedaCallUrl = "//www.mlveda.com/ShopifyApps/ShopifyMultiPricing/getLocation?shop=high-heel-jungle.myshopify.com&rnd=" + Math.floor((Math.random() * 10000000000) + 1);
      
// var syncWithThemePageFlag = false;   

  
