window.regex_mlveda_country2 = /(<(([a-z]+)\s*([^>]*))>)*mlveda_country:{0,1}(<\/\3>)*:{0,1}\s*(<(([a-z]+)\s*([^>]*))>)*\w+(<\/\8>)*/g;
